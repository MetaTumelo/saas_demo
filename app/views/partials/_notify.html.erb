<% if !alert.blank? %>
    <script>
      $(document).ready(function () {
        $.notify({
          message: "<%= alert %>"
          },{
          type: 'danger'
        });
    });
    </script>
<% end %>

<% if !notice.blank? %>
  <script>
    $(document).ready(function () {
        $.notify({
            message: "<%= notice %>"
        }, {
            type: 'success'
        });
    });
  </script>
<% end %>